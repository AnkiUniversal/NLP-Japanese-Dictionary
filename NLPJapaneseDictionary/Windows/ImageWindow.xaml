<Window x:Class="NLPJapaneseDictionary.Windows.ImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NLPJapaneseDictionary.Windows"
        mc:Ignorable="d"
        Title="ImageWindow" Height="300" Width="300">
    <Grid>
        <Image VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Stretch="Uniform" x:Name="image"/>
    </Grid>
</Window>
